<div id="leg_{{legIndex}}" class="list-group-item">
    <div class="stopover-date">
        <h4 class="stopover-month">{{date.month}}</h4>
        <h3 class="stopover-day">{{date.day}}</h3>
    </div>
    <div>
        <div class="leg-start-end">{{startAddress}}<br>{{endAddress}}</div>
        <div class="hotel-stay-list">
        {{#stay}}
            <div id="hotelStay_{{legIndex}}_{{index}}" class="hotel-stay {{icon}}"></div>
        {{/stay}}
        </div>
    </div>
</div>