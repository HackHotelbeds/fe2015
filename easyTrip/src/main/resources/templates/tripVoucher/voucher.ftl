<!-- Main component for a primary marketing message or call to action -->
<div class="jumbotron">
    <h2>Congratulations!</h2>
    <p>You have booked a great trip!</p>
</div>

<div class="voucher container-fluid vertical-center">
    <div class="col-md-8"><h3>Trip voucher by Route99</h3></div>
    <div class="col-md-4"><h4>Route99 locator: [LC123]</h4></div>

    <div class="col-md-12 supplier-result panel">
        <div class="col-md-6 panel-heading"><h4>Arrival flight</h4></div>
        <div class="col-md-12 " id="arrival-flight-selected">...flight info...</div>
    </div>

    <div class="col-md-12 supplier-result">
        <div class="col-md-6 "><h4>Car rental</h4></div>
        <div class="col-md-12 " id="car-selected">...car rental info...</div>
    </div>

    <div class="col-md-12 supplier-result">
        <div class="col-md-6 "><h4>Hotels</h4></div>
        <div class="col-md-12 " id="hotels-selected">...hotel info...</div>
    </div>

    <div class="col-md-12 supplier-result">
        <div class="col-md-6 "><h4>Tickets</h4></div>
        <div class="col-md-12 " id="tickets-selected">...ticket info...</div>
    </div>

    <div class="col-md-12 supplier-result">
        <div class="col-md-6 "><h4>Returning flight</h4></div>
        <div class="col-md-12 " id="returning-flight-selected">...flight info...</div>
    </div>
</div>
